{
  "name": "service1",
  "version": "1.0.0",
  "description": "This is a boilerplate for instrumentation using node + express service written using typescript",
  "main": "service1.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/service1.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon -q dist/service1.js\"",
    "start:service2": "cross-env PORT=8081 SERVICE_NAME=service2 node dist/service2.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@opentelemetry/auto-instrumentations-node": "^0.38.0",
    "@opentelemetry/instrumentation-winston": "^0.32.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "opentelemetry-instrumentation-express": "^0.39.1",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "typescript": "^5.1.6"
  }
}
